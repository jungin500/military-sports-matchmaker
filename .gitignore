# Node.js .gitignore files
# Logs
server/logs
server/*.log
server/npm-debug.log*
server/yarn-debug.log*
server/yarn-error.log*

# Runtime data
server/pids
server/*.pid
server/*.seed
server/*.pid.lock
server/files

# Directory for instrumented libs generated by jscoverage/JSCover
server/lib-cov

# Coverage directory used by tools like istanbul
server/coverage

# nyc test coverage
server/.nyc_output

# Grunt intermediate storage (http://gruntjs.com/creating-plugins#storing-task-files)
server/.grunt

# Bower dependency directory (https://bower.io/)
server/public/bower_components

# node-waf configuration
server/.lock-wscript

# Compiled binary addons (https://nodejs.org/api/addons.html)
server/build/Release

# Dependency directories
server/node_modules/
server/jspm_packages/

# Typescript v1 declaration files
server/typings/

# Optional npm cache directory
server/.npm

# Optional eslint cache
server/.eslintcache

# Optional REPL history
server/.node_repl_history

# Output of 'npm pack'
server/*.tgz

# Yarn Integrity file
server/.yarn-integrity

# dotenv environment variables file
server/.env

# Android .gitignore files
# Built application files
client/*.apk
client/*.ap_

# Files for the ART/Dalvik VM
client/*.dex

# Java class files
client/*.class

# Generated files
client/bin/
client/gen/
client/out/

# Gradle files
client/.gradle/
client/build/

# Local configuration file (sdk path, etc)
client/local.properties

# Proguard folder generated by Eclipse
client/proguard/

# Log Files
client/*.log

# Android Studio Navigation editor temp files
client/.navigation/

# Android Studio captures folder
client/captures/

# Intellij
client/*.iml
client/.idea/workspace.xml
client/.idea/tasks.xml
client/.idea/gradle.xml
client/.idea/dictionaries
client/.idea/libraries

# Keystore files
# Uncomment the following line if you do not want to check your keystore files in.
#*.jks

# External native build folder generated in Android Studio 2.2 and later
client/.externalNativeBuild

# Google Services (e.g. APIs or Firebase)
client/google-services.json

# Freeline
client/freeline.py
client/freeline/
client/freeline_project_description.json
